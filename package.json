{
  "name": "gameboy-commerce",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "commit": "git-cz",
    "docs": "typedoc --entryPointStrategy  expand --tsconfig ./tsconfig.json --excludeInternal --excludeProtected",
    "docs-hierarchy": "typedoc --entryPointStrategy  expand --tsconfig ./tsconfig.json --plugin ./node_modules/typedoc-theme-hierarchy/dist/index.js --theme hierarchy",
    "docs-github": "typedoc --theme github-wiki --entryPointStrategy  expand --tsconfig ./tsconfig.json  ",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/react": "6.4.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/next": "^9.0.0",
    "@types/node": "^17.0.14",
    "@types/react": "^18.0.14",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^13.0.1",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "release": "^6.3.0",
    "standard-version": "^9.5.0",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
